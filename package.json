    "@radix-ui/react-slot": "^1.2.4",
    "@react-oauth/google": "^0.12.2",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-router": "^1.134.13",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "immer": "^10.2.0",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.553.0",
    "motion": "^12.23.24",
