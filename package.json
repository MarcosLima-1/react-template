{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run script:create-images-type && vite dev --mode dev",
    "build": "vite build",
    "serve": "vite preview",
    "format": "biome format --write",
    "lint": "tsc --noEmit && biome lint",
    "check": "biome check",
    "test": "vitest run",
    "script:create-images-type": "bun ./scripts/create-images-type.ts",
    "script:optimize-images": "bun ./scripts/optimize-images.ts && bun run script:create-images-type",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o ./build/storybook",
    "docker:test": "docker compose -f ./docker/test/docker-compose.yml up --wait --wait-timeout 480 --build --remove-orphans && bun run docker:test-down",
    "docker:test-down": "docker compose -f ./docker/test/docker-compose.yml down -v",
    "docker:build": "docker compose -f ./docker/prod/docker-compose.yml up -d --build --remove-orphans",
    "docker:scan": "docker compose -f ./docker/prod/docker-compose.yml build && trivy image production-mimaeu-backend-app"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@react-oauth/google": "^0.13.4",
    "@sentry/react": "^10.35.0",
    "@sentry/vite-plugin": "^4.7.0",
    "@storybook/react-vite": "^10.1.11",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-form": "^1.27.7",
    "@tanstack/react-form-devtools": "^0.2.12",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.153.2",
    "@tanstack/react-router-devtools": "^1.153.2",
    "@tanstack/router-plugin": "^1.153.2",
    "@vitejs/plugin-react": "^5.1.2",
    "axios": "^1.13.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "immer": "^11.1.3",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "storybook": "^10.1.11",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.0.4",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@chromatic-com/storybook": "^5.0.0",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-themes": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@tanstack/devtools-event-client": "^0.4.0",
    "@types/node": "^22.19.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "4.0.17",
    "playwright": "^1.57.0",
    "react-scan": "^0.4.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook-addon-tag-badges": "^3.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^4.0.17"
  }
}
