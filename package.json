{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run script:create-images-type && vite dev --mode dev",
    "build": "vite build",
    "serve": "vite preview",
    "format": "biome format --write",
    "lint": "tsc --noEmit && biome lint",
    "check": "biome check",
    "test": "vitest run",
    "script:create-images-type": "bun ./scripts/create-images-type.ts",
    "script:optimize-images": "bun ./scripts/optimize-images.ts && bun run script:create-images-type",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o ./build/storybook",
    "docker:test": "docker compose -f ./docker/test/docker-compose.yml up --wait --wait-timeout 480 --build --remove-orphans && bun run docker:test-down",
    "docker:test-down": "docker compose -f ./docker/test/docker-compose.yml down -v",
    "docker:build": "docker compose -f ./docker/prod/docker-compose.yml up -d --build --remove-orphans",
    "docker:scan": "docker compose -f ./docker/prod/docker-compose.yml build && trivy image --format template --template '@contrib/html.tpl' production-react-template-app > ./build/trivy/scan-result.html"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@react-oauth/google": "^0.13.4",
    "@sentry/react": "^10.39.0",
    "@sentry/vite-plugin": "^4.9.1",
    "@storybook/react-vite": "^10.2.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@tanstack/react-devtools": "^0.9.5",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-form-devtools": "^0.2.16",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.160.0",
    "@tanstack/react-router-devtools": "^1.160.0",
    "@tanstack/router-plugin": "^1.160.0",
    "@vitejs/plugin-react": "^5.1.4",
    "axios": "^1.13.5",
    "babel-plugin-react-compiler": "^1.0.0",
    "dayjs": "^1.11.19",
    "immer": "^11.1.4",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.563.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.7",
    "storybook": "^10.2.8",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.1.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.3.6",
    "rollup-plugin-visualizer": "^6.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.15",
    "@chromatic-com/storybook": "^5.0.1",
    "@storybook/addon-a11y": "^10.2.8",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/addon-vitest": "^10.2.8",
    "@tanstack/devtools-event-client": "^0.4.0",
    "@types/node": "^22.19.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "consola": "^3.4.2",
    "jsdom": "^27.4.0",
    "playwright": "^1.58.2",
    "react-scan": "^0.4.3",
    "storybook-addon-tag-badges": "^3.0.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^4.0.18"
  }
}
