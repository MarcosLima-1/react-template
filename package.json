{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run script:create-images-type && vite dev --mode dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "tsc --noEmit && vitest run",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check",
    "script:create-images-type": "bun ./scripts/create-images-type.ts",
    "script:optimize-images": "bun ./scripts/optimize-images.ts && bun run script:create-images-type",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o ./build/storybook",
    "docker:test": "docker compose -f ./docker/test/docker-compose.yml up --wait --wait-timeout 120 --build --remove-orphans",
    "docker:build": "docker compose -f ./docker/prod/docker-compose.yml up -d --build --remove-orphans"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@react-oauth/google": "^0.12.2",
    "@storybook/react-vite": "^10.0.8",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/devtools-vite": "^0.3.11",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-form-devtools": "^0.2.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.91.0",
    "@tanstack/react-router": "^1.139.10",
    "@tanstack/react-router-devtools": "^1.139.10",
    "@tanstack/router-plugin": "^1.139.10",
    "@vitejs/plugin-react": "^5.1.1",
    "axios": "^1.13.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.19",
    "immer": "^11.0.0",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.555.0",
    "motion": "^12.23.24",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sonner": "^2.0.7",
    "storybook": "^10.0.8",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vite-tsconfig-paths": "^5.1.4",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.0.8",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/addon-themes": "^10.0.8",
    "@storybook/addon-vitest": "^10.0.8",
    "@tanstack/devtools-event-client": "^0.3.5",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "4.0.14",
    "@vitest/coverage-v8": "4.0.14",
    "jsdom": "^27.2.0",
    "playwright": "^1.56.1",
    "react-scan": "^0.4.3",
    "rollup-plugin-visualizer": "^6.0.5",
    "storybook-addon-tag-badges": "^3.0.2",
    "terser": "^5.44.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^4.0.12"
  }
