{
  "name": "react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run script:create-images-type && vite dev --mode dev",
    "build": "vite build",
    "serve": "vite preview",
    "test": "bunx run tsc --noEmit && vitest run",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check",
    "script:create-images-type": "bun ./scripts/create-images-type.ts",
    "script:optimize-images": "bun ./scripts/optimize-images.ts && bun run script:create-images-type",
    "docker:test": "docker compose -f docker-compose.test.yml up --wait --wait-timeout 120 --build --remove-orphans",
    "docker:build": "docker compose -f docker-compose.yml up"
  },
  "dependencies": {
